{
  "name": "react-antd-skeleton",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^4.1.2",
    "axios": "^0.19.2",
    "customize-cra": "^0.9.1",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "lodash-decorators": "^6.0.1",
    "lodash.throttle": "^4.1.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-focus-lock": "^2.2.1",
    "react-parallax": "^3.0.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-hash-link": "^1.2.2",
    "react-scripts": "3.4.0",
    "styled-components": "^5.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "babel-plugin-import": "^1.13.0",
    "env-cmd": "^10.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "jest-canvas-mock": "^2.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "react-test-renderer": "^16.12.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "verbose": true
  },
  "scripts": {
    "start": "export PORT=3001 && react-app-rewired start",
    "start:debug": "export PORT=3001 && REACT_APP_LOG_LEVEL=debug react-app-rewired start",
    "start:staging": "env-cmd -f .env.staging npm run start",
    "build": "react-app-rewired build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "defaults"
  ]
}
